{
  "name": "alexbracken/content-gate",
  "description": "Protect WordPress post content with a simple identification form",
  "keywords": ["wordpress", "plugin", "roots"],
  "homepage": "https://github.com/alexbracken/content-gate",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Bracken",
      "email": "hello@alexbracken.co",
      "homepage": "https://alexbracken.co"
    }
  ],
  "type": "wordpress-plugin",
  "require": {
    "php": ">=8.0",
    "composer/installers": "^2.3"
  },
  "require-dev": {
      "friendsofphp/php-cs-fixer": "^3.75",
      "squizlabs/php_codesniffer": "^3.7",
      "wp-coding-standards/wpcs": "^3.0"
  },
  "config": {
      "allow-plugins": {
          "composer/installers": true,
          "dealerdirect/phpcodesniffer-composer-installer": true
      }
  }
}
